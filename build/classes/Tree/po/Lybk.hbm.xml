<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="Tree.po">
	<class name="Lybk" table="lybk">
		<id name="id" type="integer" column="id">
			<generator class="increment"/>
		</id> 
		<property name="wordName" column="wordname" type="string"/>
		<property name="content" column="content" type="text"/>    
		<property name="urlAddress" column="urladdress" type="string"/> 
		<property name="type" column="type" type="integer"/> 
	</class>
</hibernate-mapping>